{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "customManagers": [
    {
      "customType": "regex",
      "fileMatch": "dependency",
      "matchStrings": [
        "^(?<depName>[^ ]+)\\s+(?<currentValue>[^ ]+)\\s+(?<currentDigest>[0-9a-f]+)"
      ],
      "datasourceTemplate": "github-release-attachments",
    },
  ],
}
