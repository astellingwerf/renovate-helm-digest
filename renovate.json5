{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "customManagers": [
    {
      "customType": "regex",
      "fileMatch": "dependency",
      "matchStrings": [
        "^(?<depName>.*)\\s+(?<currentValue>.*)\\s+(?<currentDigest>.*)\\n"
      ],
      "datasourceTemplate": "github-release-attachments",
    },
  ],
}
